{
  "name": "design-token-dealer",
  "version": "0.1.0",
  "description": "open source design token distribution package for react projects ",
  "keywords": [
    "design",
    "token"
  ],
  "homepage": "https://github.com/tdsfugal/design-token-dealer#readme",
  "bugs": {
    "url": "https://github.com/tdsfugal/design-token-dealer/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tdsfugal/design-token-dealer.git"
  },
  "license": "MIT",
  "author": "Tim Smith",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "peerDependencies": {
    "react": "^19.2.3"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
